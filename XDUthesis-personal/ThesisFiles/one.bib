@phdthesis{wujingshuma,
	author = {黄幼萍},
	title = {数码裂隙灯显微成像光学系统设计},
	university = {福建师范大学},
	type = {硕士},
	abstract = {裂隙灯显微镜广泛应用于眼视光学领域,随着数码成像技术、计算机技术和图像处理技术的不断完善,大力推动了裂隙灯显微镜技术的进步,各种系列的裂隙灯显微镜层出不穷。传统的裂隙灯显微镜仅能用眼睛观察,长时间观测会带来视觉疲劳,且分辨率低下,影响诊断结果,而数码裂隙灯显微镜可利用计算机进行动态观察,具有高分辨率成像、快速捕捉影像,随时储存信息等特性,使得它成为业界人士研究的热点。为了提高分辨率,本论文采用平行式伽利略望远系统的方式设计具有5档有级变倍、CCD内置式一体化的数码裂隙灯显微镜的光学系统,主要完成以下几个工作：1、根据数码裂隙灯显微镜光学系统设计的成像原理,把整个系统分为前置物镜、伽利略望远系统和摄影物镜三个结构,分别研究各自的光学特性。2、推导出变倍系统的倍率以及每一个结构的设计指标,确定整个系统的外形尺寸。3、利用Zemax的多重结构同时对5档放大倍率的不同镜片组进行像差优化设计。先进行与前置物镜共轴状态下的优化设计,在共轴系统取得良好性能的基础上,将伽利略系统的光轴在-Y方向上进行平移,实现非共轴的优化设计。},
	keywords = {数码裂隙灯显微镜
	光学系统设计
	Zemax多重结构
	一体化
	非共轴},
	year = {2015},
	type = {Thesis}
}

